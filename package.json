{
    "name": "sholtomaud",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sholtomaud/sholtomaud.git"
    },
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "build:data": "cat ./public/*.pdf  | jq  --raw-input .  | jq --slurp . > ./src/data.json",
        "build:latex": "cd latex/experiments && buildtex main.tex --debug --date --version && mv *.pdf ../../public && cd ../../ ",
        "preview": "vite preview"
    },
    "devDependencies": {
        "@types/html-minifier": "^4.0.2",
        "html-minifier": "^4.0.0",
        "sass": "^1.54.5",
        "typescript": "^4.6.4",
        "vite": "^3.0.7"
    }
}
